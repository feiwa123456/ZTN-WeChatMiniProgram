<view><view class="cu-custom" style="{{'height:'+(CustomBar+'px')+';'}}"><view class="{{['cu-bar fixed',bgImage!=''?'none-bg text-white bg-img':'']}}" style="{{(style)}}"><block wx:if="{{isBack}}"><view data-event-opts="{{[['tap',[['BackPage',['$event']]]]]}}" class="action" style="{{'color:'+(textColor?textColor:themei18n.navigationBarTextColor)+';'}}" bindtap="__e"><text class="cuIcon-back"></text><slot name="backText"></slot></view></block><block wx:if="{{isHome}}"><view data-event-opts="{{[['tap',[['toHome',['$event']]]]]}}" class="action" style="{{'color:'+(textColor?textColor:themei18n.navigationBarTextColor)+';'}}" bindtap="__e"><text class="cuIcon-homefill"></text><slot name="homeText"></slot></view></block><block wx:if="{{isCustom}}"><view data-event-opts="{{[['tap',[['parentEvent',['$event']]]]]}}" class="action" style="{{'color:'+(themei18n.navigationBarHighlightColor)+';'}}" bindtap="__e"><text class="{{[customIcon]}}" style="font-size:50rpx;font-weight:bold;"></text><slot name="customText"></slot></view></block><view class="content" style="{{'top:'+(StatusBar+'px')+';'}}"><slot name="content"></slot></view><slot name="right"></slot></view></view></view>