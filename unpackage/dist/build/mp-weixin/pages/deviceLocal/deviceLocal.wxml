<view><block wx:if="{{isToHome}}"><cu-custom vue-id="4aa498d4-1" isHome="{{true}}" bind:__l="__l" vue-slots="{{['homeText']}}"><view class="text-white" style="{{'color:'+(navigationBarTextColor)+';'}}" slot="homeText">{{deviceName}}</view></cu-custom></block><block wx:else><cu-custom vue-id="4aa498d4-2" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText']}}"><view class="text-white" style="{{'color:'+(navigationBarTextColor)+';'}}" slot="backText">{{deviceName}}</view></cu-custom></block><block wx:if="{{isShowMap}}"><map style="width:100%;height:400rpx;" id="myMap" scale="{{scale}}" longitude="{{longitude}}" latitude="{{latitude}}" markers="{{markers}}"></map></block><scroll-view style="{{(scrollStyle)}}" scroll-y="true"><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-timeline"><block wx:if="{{item.$orig.status=='OFFLINE'}}"><view class="cu-time">{{i18n.precision+":"+item.$orig.radius}}</view></block><view class="cu-item cuIcon-attentionfill" style="{{'color:'+(lightColor)+';'}}"><view class="content" style="background-color:#f8f8f8;"><view class="flex justify-between align-center"><view data-event-opts="{{[['tap',[['tapMarker',['$0'],[[['deviceLocalList','',index]]]]]]]}}" style="flex-basis:90%;" bindtap="__e"><view><text class="text-gray">{{i18n.serial+":"+(index+1)}}</text><text class="text-gray margin-left">{{item.f0}}</text></view><view style="color:#666666;">{{item.$orig.address}}</view></view><view data-event-opts="{{[['tap',[['navigation',['$0'],[[['deviceLocalList','',index]]]]]]]}}" bindtap="__e"><text class="cuIcon-footprint" style="{{'font-size:44rpx;'+('color:'+(emphasizeColor)+';')}}"></text></view></view></view></view></view></block></scroll-view><view class="{{['cu-modal',isShowAuthorizeModal?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-white justify-end"><view class="content">{{i18n.authorize}}</view><view data-event-opts="{{[['tap',[['hideAuthorizeModal',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="padding-xl">{{''+i18n.leadGoAuthorize+''}}</view><view class="cu-bar bg-white justify-end"><view class="action"><button data-event-opts="{{[['tap',[['hideAuthorizeModal',['$event']]]]]}}" class="cu-btn line-green text-green" style="{{'border-color:'+(emphasizeColor)+';'+('color:'+(emphasizeColor)+';')}}" bindtap="__e">{{i18n.cancel}}</button><button class="cu-btn bg-green margin-left" style="{{'background-color:'+(emphasizeColor)+';'}}" open-type="openSetting" data-event-opts="{{[['opensetting',[['authorizeManage',['$event']]]]]}}" bindopensetting="__e">{{i18n.confirm}}</button></view></view></view></view></view>